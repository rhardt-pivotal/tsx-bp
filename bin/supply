#!/bin/sh
echo "\n\n***\nHELLO FROM SUPPLY!!!!\n***\n\n $1   :   $2   :   $3   :   $4"
mkdir $3/$4/bin
mkdir $3/bin
touch $3/bin/three.sh
touch $3/$4/threefour.sh
CONTENT=$(cat <<EOF
java_opts:
  system_properties: -Dhello=true
EOF
)
echo $CONTENT
echo $CONTENT > $3/$4/config.yml
echo $CONTENT > $3/config.yml
